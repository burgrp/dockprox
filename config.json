{
    "module": "./proxy.js",
    "http": {
        "enabled": "-> $.HTTP_ENABLED === undefined? true: $.HTTP_ENABLED",
        "port": "-> $.HTTP_PORT || 80"
    },
    "https": {
        "enabled": "-> $.HTTPS_ENABLED || false",
        "port": "-> $.HTTPS_PORT || 443",
        "key": "-> $.HTTPS_PRIVKEY || '/data/privkey.pem'",
        "cert": "-> $.HTTPS_CERT || '/data/cert.pem'",
        "ca": "-> $.HTTPS_CA",
        "requestCert": "-> $.HTTPS_REQUEST_CERT || false",
        "rejectUnauthorized": "-> $.HTTPS_REJECT_UNAUTHORIZED || false",
        "ctxMapping": "-> $.HTTPS_CTX_MAPPING"
    },
    "mapper": {
        "module": "./docker.js",
        "socket": "-> $.DOCKER_SOCKET || '/var/run/docker.sock'",
        "domain": "-> $DOMAIN",
        "remapToLocalhost": "-> $.REMAP_TO_LOCALHOST"
    }
}